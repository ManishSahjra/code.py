    days = ["Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday"]

    if n2 == 29:
        result1 = calendar.weekday(n3, n1, n2 - 6)
        print(days[int(result1)].upper())
